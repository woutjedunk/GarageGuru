{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2016",
        "forceConsistentCasingInFileNames": true,
        // "strict": true,
        "esModuleInterop": true,
        "rootDir": "./src",
        "outDir": "./dist",
        "baseUrl": "./src",
        "paths": {
            "#model/*": ["domain/model/*"],
            "#data-acces/*": ["domain/data-acces/*"],
            "#service/*": ["service/*"],
            "#controller/*": ["controller/*"],
            "#util/*": ["util/*"]
        }
    }
}